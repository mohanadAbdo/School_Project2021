@model List<ContactUs>
<table class="table table-striped">
    <thead>
        <tr>
            <th scope="col">#</th>
            <th scope="col">Name</th>
            <th scope="col">Email</th>
            <th scope="col">Title</th>
            <th scope="col">Message</th>
        </tr>
    </thead>
    <tbody>
        @for (int i = 0; i < Model.Count; i++)
        {
        <tr>
            <th scope="row">@(i+1)</th>
            <td>@Model[i].Name</td>
            <td>@Model[i].Email</td>
            <td>@Model[i].Title</td>
            <td>@Model[i].Message</td>
            <td>
                <a asp-action="Delete_Contact" asp-route-id="@Model[i].ID" class="btn btn-danger">Delete</a>
            </td>
        </tr>
        }
    </tbody>
</table>